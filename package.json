{
  "name": "mf-hermex",
  "version": "1.0.0",
  "description": "Micro FrontEnd de Notificaciones (Hermex)",
  "private": true,
  "scripts": {
    "webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
    "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
    "webpack-qas": "cross-env NODE_ENV=test webpack --mode none",
    "build:dev": "webpack-dev-server --inline --hot --progress --port 9075",
    "build:prod": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
    "build:qas": "npm run build:clean && ngc && npm run webpack-qas && npm run build:clean",
    "build:clean": "del-cli src/*.js src/*.js.map src/*.ngsummary.json src/**/*.js src/**/*.js.map src/**/*.ngsummary.json src/**/*.metadata.json src/**/**/*.ngfactory.ts src/**/*.ngstyle.ts src/**/*.shim.ts",
    "serve": "lite-server"
  },
  "author": "mihernandez@personalsoft.com.co",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2",
    "@angular/compiler": "~7.2",
    "@angular/compiler-cli": "~7.2",
    "@angular/core": "~7.2",
    "@angular/forms": "~7.2",
    "@angular/http": "~7.2",
    "@angular/platform-browser": "~7.2",
    "@angular/platform-browser-dynamic": "~7.2",
    "@angular/platform-server": "~7.2",
    "@angular/router": "~7.2",
    "@angular/upgrade": "~7.2",
    "@aspnet/signalr": "^1.1.4",
    "@auth0/angular-jwt": "^3.0.0",
    "@types/lodash": "^4.14.134",
    "bootstrap": "^4.5.0",
    "bootstrap-loader": "1.2.1",
    "core-js": "~2.6",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "jasmine": "^3.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "ngx-pagination": "^5.0.0",
    "ngx-toastr": "^12.1.0",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.3",
    "rxjs": "~6.4",
    "rxjs-observable": "0.0.7",
    "single-spa": "4.4.1",
    "single-spa-angular": "3.0.1",
    "url-loader": "^1.1.2",
    "zone.js": "~0.8"
  },
  "devDependencies": {
    "@types/core-js": "~2.5",
    "@types/node": "^10.12.30",
